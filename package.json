{
	"name": "nseed",
	"version": "1.0.0",
	"description": "A NodeJS CLI Tool for database seeding with auto-generated data.",
	"main": "lib/main.min.js",
	"types": "lib/main.min.d.ts",
	"bin": {
		"nSeed": "lib/main.min.js",
		"nseed": "lib/main.min.js"
	},
	"publishConfig": {
		"access": "public",
		"tag": "1.0.0"
	},
	"scripts": {
		"test": "jest",
		"start": "cd examples && node --trace-warnings ../lib/main.min.js",
		"standard": "ts-standard",
		"standard:fix": "npm run standard --fix",
		"commit": "npx cz",
		"build:dev": "rollup -c -w",
		"build:prod": "set NODE_ENV=production&& rollup -c"
	},
	"type": "commonjs",
	"repository": {
		"type": "git",
		"url": "https://github.com/F2BEAR/nSeed"
	},
	"bugs": {
		"url": "https://github.com/F2BEAR/nSeed/issues"
	},
	"homepage": "https://github.com/F2BEAR/nSeed",
	"keywords": [
		"cli",
		"nseed",
		"mongodb",
		"seeder",
		"db",
		"seed",
		"db-seeding"
	],
	"author": "Facundo Carbonel",
	"license": "MIT",
	"dependencies": {
		"@faker-js/faker": "7.2.0",
		"arg": "^5.0.1",
		"cli-table": "^0.3.6",
		"inquirer": "^8.2.0",
		"mongodb": "^4.1.3",
		"progress": "^2.0.3"
	},
	"devDependencies": {
		"@rollup/plugin-typescript": "^8.3.3",
		"@types/cli-table": "^0.3.0",
		"@types/inquirer": "^8.2.1",
		"@types/jest": "^27.5.2",
		"@types/node": "^17.0.23",
		"@types/progress": "^2.0.5",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "^3.0.1",
		"jest": "^27.5.1",
		"jest-mock-process": "^2.0.0",
		"mock-argv": "^2.0.8",
		"mock-cwd": "^1.0.0",
		"rollup": "^2.76.0",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-dts": "^4.2.2",
		"rollup-plugin-license": "^2.8.1",
		"rollup-plugin-preserve-shebang": "^1.0.1",
		"rollup-plugin-terser": "^7.0.2",
		"terser": "^5.14.1",
		"ts-jest": "^27.1.5",
		"ts-node": "^10.7.0",
		"ts-standard": "^11.0.0",
		"typescript": "^4.6.3"
	},
	"ts-standard": {
		"ignore": [
			"/__tests__/",
			"/lib/",
			"/examples/",
			"jest.config.ts",
			"rollup.config.js"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
